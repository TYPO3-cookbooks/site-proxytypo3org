{
  "name": "typo3.org",
  "nginx": {
    "backend": "http://srv107.typo3.org:80",
    "options": {
      "add_header": {
        "Strict-Transport-Security": "\"max-age=15768000; preload;\""
      },
      "locations": {
        "/api/typo3cms": {
          "proxy_pass":  "http://api.typo3.org/typo3cms/current/html",
          "proxy_set_header": "Host api.typo3.org"
        }
      }
    }
  }
}